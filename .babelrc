{
  "presets": [
    [
      "@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["babel-plugin-styled-components", {}, "plugin-styled-components"],
    ["@babel/plugin-proposal-object-rest-spread", {}, "plugin-object-rest"],
    ["@babel/plugin-proposal-class-properties", {}, "plugin-class-properties"],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      },
      "transform-runtime"
    ]
  ]
}
